dev tun
persist-tun
persist-key
data-ciphers AES-256-GCM:AES-128-GCM:CHACHA20-POLY1305:AES-256-CBC
data-ciphers-fallback AES-256-CBC
auth SHA256
tls-client
client
resolv-retry infinite
remote crtgroup.dyndns.org 7070 udp4
nobind
auth-user-pass
ca pfSense-UDP4-7070-ca.crt
tls-auth pfSense-UDP4-7070-tls.key 1
remote-cert-tls server
explicit-exit-notify

